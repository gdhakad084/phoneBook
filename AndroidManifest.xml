<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phonebook"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.phonebook.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.phonebook.ContactPage"
            android:label="@string/title_activity_contact_page" >
        </activity>
        <activity
            android:name="com.example.phonebook.KeypadPage"
            android:label="@string/title_activity_keypad_page" >
        </activity>
        <activity
            android:name="com.example.phonebook.Favourite"
            android:label="@string/title_activity_favourite" >
        </activity>
        <activity
            android:name="com.example.phonebook.DisplayASingleRecord"
            android:label="@string/title_activity_display_asingle_record" >
        </activity>
        <activity
            android:name="com.example.phonebook.FavView"
            android:label="@string/title_activity_fav_view" >
        </activity>
    </application>

</manifest>